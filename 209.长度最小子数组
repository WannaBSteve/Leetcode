class Solution {
public:
    int minSubArrayLen(int target, vector<int>& nums) {
        int res=INT32_MAX;
        int sum=0,i=0;
        for(int j=0;j<nums.size();j++)
        {
            sum+=nums[j];
            while(sum>=target)
            {
                res=min(res,j-i+1);
                sum-=nums[i];
                i++;
            }
        }
        return res==INT32_MAX?0:res;
    }
};
//滑动窗口法
